package edu.mission;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.FileReader;
import java.util.StringTokenizer;

public class Mission9_2 {

    public static void main(String[] args) throws IOException {

        BufferedReader br = new BufferedReader(new FileReader("input.txt"));
        StringTokenizer st = new StringTokenizer(br.readLine());

        String a = st.nextToken();
        String b = st.nextToken();

        int max = a.length() > b.length() ? a.length() : b.length();

        char[] arr1 = new char[max];
        char[] arr2 = new char[max];

        //String a, b를 char 배열로 바꾸어 저장
        if (max == a.length()) {
            arr1 = a.toCharArray();
            int i = 0;
            for (; i < max - b.length(); i++) {
                arr2[i] = '0';
            }
            for (; i < max; i++) {
                arr2[i] = b.charAt(i - (max - b.length()));
            }
        }
        else {
            arr2 = b.toCharArray();
            int i = 0;
            for (; i < max - a.length(); i++) {
                arr1[i] = '0';
            }
            for (; i < max; i++) {
                arr1[i] = a.charAt(i - (max - a.length()));
            }
        }

        //더하고 그 결과를 sb에 저장
        int tmp = 0;
        StringBuilder sb = new StringBuilder();
        for (int i = max -1; i>=0; i--) {
            int num = arr1[i] - '0' + arr2[i] - '0' + tmp;
            if (num >= 10) {
                tmp = 1;
                num %= 10;
            }
            else tmp = 0;
            sb.insert(0, num);
        }

        if (tmp == 1)
            sb.insert(0, 1);

        System.out.println(sb);

    }
}