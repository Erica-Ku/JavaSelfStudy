package edu.mission;

public class Mission7 {

	public static void printGugudan(int n) {
		System.out.println(n + "ë‹¨");
			for(int i = 1; i <= 9 ; i++) {
				System.out.println(n + "*" + i + "=" + (n*i));
		}
		System.out.println();
	}
	
	public static void main(String[] args) {
	
		printGugudan(1);
		
	}
}