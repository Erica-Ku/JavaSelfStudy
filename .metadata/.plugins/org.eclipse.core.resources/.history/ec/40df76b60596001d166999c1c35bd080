package edu.mission;

//import java.util.Scanner;
public class Mission3 {

	public static void main(String[] args) {
		//Scanner sc = new Scanner(System.in);
		int n = 13;
		int a;

		while(true) {
			a = n % 2;
			System.out.print(a);
			n /= 2;
			if(n < 2) {
				System.out.print(n);
				break;
			}
		}
		//sc.close();
	}
}