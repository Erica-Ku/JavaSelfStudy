package Chap5_재귀알고리즘;

import Chap5_재귀알고리즘.Maze.items;

public class MazeStack_1 {
	private int top;
	private items [] data;
	
	public MazeStack_1(int i) {
		top = 0;
		data = new items[i];
	}
	
	public void push(items p) {
		data[top++] = p;
	}
	
	public items pop() {
		return data[--top];
	}
	
	public boolean isEmpty() {
        if (top == 0)
            return true;
        else
            return false;
    }
}